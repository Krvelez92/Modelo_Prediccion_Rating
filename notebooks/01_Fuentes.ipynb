    "])\n",
    "\n",
    "detalle_sitios = []\n",
    "\n",
    "for i in lugares['id']:\n",
    "    url = 'https://maps.googleapis.com/maps/api/place/details/json'\n",
    "    \n",
    "    params = {'place_id': i,\n",
    "            'fields': fields,\n",
    "            'key':config['access_key']}\n",
    "\n",
    "    details = requests.get(url, params=params)\n",
    "    resultado = details.json().get('result', {})\n",
    "    detalle_sitios.append(resultado)"
